  <>\r\n        <IconButton color=\"inherit\" onClick={() => setOpen(true)}>\r\n          <Badge badgeContent={numOfReq} color=\"secondary\">\r\n            <NotificationsIcon />\r\n          </Badge>\r\n        </IconButton>\r\n        <Dialog\r\n          open={open}\r\n          onClose={() => setOpen(false)}\r\n          aria-labelledby=\"alert-dialog-title\"\r\n          aria-describedby=\"alert-dialog-description\"\r\n          fullWidth\r\n        >\r\n          {numOfReq === 0 ? (\r\n            <DialogTitle id=\"alert-dialog-title\">\r\n              You have no request\r\n            </DialogTitle>\r\n          ) : (\r\n            <DialogTitle id=\"alert-dialog-title\">Requests</DialogTitle>\r\n          )}\r\n\r\n          <DialogContent>\r\n            <List>\r\n              {requestList.map((requestUser) => (\r\n                <RequestItem\r\n                  key={requestUser.uid}\r\n                  currentUser={currentUser}\r\n                  requestUser={requestUser}\r\n                />\r\n              ))}\r\n            </List>\r\n          </DialogContent>\r\n          <DialogActions>\r\n            <Button onClick={() => setOpen(false)} color=\"primary\" autoFocus>\r\n              Ok\r\n            </Button>\r\n          </DialogActions>\r\n        </Dialog>\r\n      </>\r\n    );\r\n  } else {\r\n    return null;\r\n  }\r\n};\r\n\r\nexport default FriendRequestDialog;\r\n","c:\\projects\\react-emessage\\client\\src\\components\\SuccessMessage\\SuccessMessage.jsx",[],"c:\\projects\\react-emessage\\client\\src\\components\\RequestItem\\RequestItem.jsx",[],"c:\\projects\\react-emessage\\client\\src\\pages\\SignUpPage\\SignUpPage.jsx",[],"c:\\projects\\react-emessage\\client\\src\\pages\\MessagePage\\MessagePage.jsx",[],"c:\\projects\\react-emessage\\client\\src\\pages\\NotFoundPage\\NotFoundPage.jsx",[],"c:\\projects\\react-emessage\\client\\src\\components\\ContactPanel\\ContactPanel.jsx",["127"],"import React, { useState, useEffect } fr