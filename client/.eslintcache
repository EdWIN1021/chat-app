ord\"\r\n            type=\"password\"\r\n            id=\"password\"\r\n            value={input.password}\r\n            autoComplete=\"current-password\"\r\n            onChange={handleOnChange}\r\n          />\r\n\r\n          <Button\r\n            type=\"submit\"\r\n            fullWidth\r\n            variant=\"contained\"\r\n            color=\"primary\"\r\n            className={classes.submit}\r\n          >\r\n            Sign In\r\n          </Button>\r\n          <Box textAlign=\"center\">\r\n            Don't have an account?<Link to=\"/signup\"> Sign Up</Link>\r\n          </Box>\r\n        </form>\r\n      </div>\r\n    </Container>\r\n  );\r\n};\r\n// };\r\n\r\nexport default SignInPage;\r\n","C:\\projects\\react-emessage\\client\\src\\components\\HeaderMenu\\HeaderMenu.jsx",[],"C:\\projects\\react-emessage\\client\\src\\components\\MenuProfileDialog\\ProfileDialog.jsx",[],["159","160"],"C:\\projects\\react-emessage\\client\\src\\pages\\SignUpPage\\SignUpPage.jsx",[],"C:\\projects\\react-emessage\\client\\src\\components\\Loader\\Loader.styles.jsx",[],"C:\\projects\\react-emessage\\client\\src\\Validator\\validator.js",[],["161","162"],"C:\\projects\\react-emessage\\client\\src\\pages\\SignInPage\\SignInPage.styles.jsx",[],"C:\\projects\\react-emessage\\client\\src\\components\\Copyright\\Copyright.jsx",[],["163","164"],"C:\\projects\\react-emessage\\client\\src\\pages\\NotFoundPage.jsx",[],"C:\\projects\\react-emessage\\client\\src\\components\\message.box\\MessageBox.jsx",["165","166"],"C:\\projects\\react-emessage\\client\\src\\components\\ErrorMessage\\ErrorMessage.styles.jsx",[],["167","168"],"C:\\projects\\react-emessage\\client\\src\\pages\\SignUpPage\\SignUpPage.styles.jsx",[],"C:\\projects\\react-emessage\\client\\src\\index.js",[],"C:\\projects\\react-emessage\\client\\src\\redux\\store.js",[],{"ruleId":"169","severity":1,"message":"170","line":5,"column":8,"nodeType":"171","messageId":"172","endLine":5,"endColumn":19},{"ruleId":"173","sev